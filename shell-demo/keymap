#---------------------------
# 终端快捷键设置
# 用下面这些alias作为你的快捷键
# 设置方式
# 到系统的全局变量文件夹下：/etc/profile  的末尾追加 source ./keymap 然后执行source /etc/profile 即可生效
#----------------------------

# maven

alias mvndeploy='mvn -Dmaven.test.skip=true clean deploy'

alias mvninstall='mvn -Dmaven.test.skip=true clean install'

alias mvncompile='mvn -Dmaven.test.skip=true clean compile'


# git

alias gitstatus='git status -s'

alias gitupdate='git commit -a -m codeUpdate'

alias gitcodeBackUp='git commit -a -m codeBackUp'

alias gitpush='git push'

alias gitcommit='git commit -a -m'

alias gitpull='git pull'

alias gitrm='git rm -rf  --cached'

alias gitadd='git add  -A '

alias gitcheckout='git checkout '

alias gitbranch='git branch'

# file

alias ll='ls -1ld *'

alias la='ls -1a'

# vim

alias vi='vim'


# brew

alias brewinstall='brew install'

alias brewuninstall='brew uninstall'

alias brewlist='brew list'
